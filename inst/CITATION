citEntry(entry="misc",
         title = "PogromcyDanych R package",
         author = personList(
	   person(given ="Przemyslaw", family="Biecek", email = "Przemyslaw.Biecek@gmail.com")
         ),
	 journal = "",
	 year = "2015",
         textVersion =
         paste("Przemyslaw Biecek (2015)",
               "PogromcyDanych R package",
               sep=", ")
         )


